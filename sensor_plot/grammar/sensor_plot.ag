(struct WSF_DUMMY_WEAPON :base_type Weapon (type weaponType WSF_DUMMY_WEAPON)
{
   <ignore>
})
(struct WSF_DUMMY_WEAPON_EFFECTS :base_type WSF_EXPLICIT_WEAPON_EFFECT (type weaponEffect WSF_DUMMY_WEAPON_EFFECTS)
