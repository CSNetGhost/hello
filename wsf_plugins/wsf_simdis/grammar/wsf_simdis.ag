#==============================================================================
# File WsfSIMDIS_Interface.cpp

(rule FHN { F | H | N } )

(rule simdis-interface-command
{
   file (file-reference asi_file)
 | hit_icon (name icon)
 | kill_icon (name icon)
 | event_duration <Time>
 | enable_beams <Bool>
 | default_beam_color (name color)
 | beam_color (typeref sensorType) (name color)
 | reference_lla <Latitude> <Longitude> <Length>
 | define_fhn <string> <FHN>
})

(rule root-command {
   simdis_interface <simdis-interface-command>* end_simdis_interface
})
